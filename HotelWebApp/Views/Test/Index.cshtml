
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <ul>
        @foreach (var comp in ViewBag.list)
        {
            <li>@comp.vendor_company</li>
            <li>@comp.vendor_name</li>
            <li>@comp.vendor_address</li>
            <li>@comp.vendor_phone</li>
            <li>@comp.join_date</li>
        }
    </ul>
</body>
</html>
